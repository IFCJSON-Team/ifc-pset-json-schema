{
    "$id": "Pset_AirTerminalOccurrence.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "ifcversion": "IFC4x2",
    "type": "object",
    "title": "Pset_AirTerminalOccurrence",
    "description": "Air terminal occurrence attributes attached to an instance of IfcAirTerminal.",
    "properties": {
        "AirflowType": {
            "title": "AirflowType",
            "description": "Enumeration defining the functional type of air flow through the terminal.",
            "datatype": {
                "title": null,
                "description": null,
                "type": "string",
                "enum": [
                    "SUPPLYAIR",
                    "RETURNAIR",
                    "EXHAUSTAIR",
                    "OTHER",
                    "NOTKNOWN",
                    "UNSET"
                ]
            }
        },
        "AirFlowRate": {
            "title": "AirFlowRate",
            "description": "The actual airflow rate as designed.",
            "datatype": "IfcVolumetricFlowRateMeasure"
        },
        "Location": {
            "title": "Location",
            "description": "Location  (a single type of diffuser can be used for multiple locations); high means close to ceiling.",
            "datatype": {
                "title": null,
                "description": null,
                "type": "string",
                "enum": [
                    "SIDEWALLHIGH",
                    "SIDEWALLLOW",
                    "CEILINGPERIMETER",
                    "CEILINGINTERIOR",
                    "FLOOR",
                    "SILL",
                    "OTHER",
                    "NOTKNOWN",
                    "UNSET"
                ]
            }
        }
    }
}