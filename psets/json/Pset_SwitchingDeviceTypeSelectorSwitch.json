    "ifcversion": "IFC4x2",
    "type": "object",
    "title": "Pset_SwitchingDeviceTypeSelectorSwitch",
    "description": "A selector switch is a switch that adjusts electrical power through a multi-position action.  HISTORY: Added in IFC4.",
    "properties": {
        "SelectorType": {
            "title": "SelectorType",
            "description": "A list of the available types of selector switch from which that required may be selected.",
            "datatype": {
                "title": null,
                "description": null,
                "type": "string",
                "enum": [
                    "BUTTONS",
                    "SLIDE",
                    "TWIST",
                    "OTHER",
                    "NOTKNOWN",
                    "UNSET"
                ]
            }
        },
        "SwitchUsage": {
            "title": "SwitchUsage",
            "description": "A list of the available usages for selector switches from which that required may be selected.",
            "datatype": {
                "title": null,
                "description": null,
                "type": "string",
                "enum": [
                    "EMERGENCY",
                    "GUARD",
                    "LIMIT",
                    "START",
                    "STOP",
                    "OTHER",
                    "NOTKNOWN",
                    "UNSET"
                ]
            }
        },
        "SwitchActivation": {
            "title": "SwitchActivation",
            "description": "A list of the available activations for selector switches from which that required may be selected.",
            "datatype": {
                "title": null,
                "description": null,
                "type": "string",
                "enum": [
                    "ACTUATOR",
                    "FOOT",
                    "HAND",
                    "PROXIMITY",
                    "SOUND",
                    "TWOHAND",
                    "WIRE",
                    "OTHER",
                    "NOTKNOWN",
                    "UNSET"
                ]
            }
