{
    "$id": "Pset_AudioVisualApplianceTypeProjector.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "ifcversion": "IFC4x2",
    "type": "object",
    "title": "Pset_AudioVisualApplianceTypeProjector",
